"VertexLitGeneric"
{
	"$basetexture" "models/weapons/c_models/c_syringe_medigun/c_syringe_medigun"
	
			 	"$bumpmap" "water/dx80_tfwater001_normal"

	"$bumpmap" "water/tfwater001_normal"
	"$detail" "models/player/shared/red_uber"
	"$detailblendfactor" "0.76"
	"$envmap" "effects/invun_red"
	"$surfaceprop" "metal"
	"$halflambert" "1"
	
	"$basemapalphaphongmask" "1"
	"$phong" "1"
	"$phongboost" "0.5"
	"$phongexponent" "25"
	"$phongfresnelranges"	"[.5 1.5 10]"
	
	"$selfillum" "1"
	"$selfillumtint" "[0.7 0.3 0.3]"
	"selfillummaskscale" "10"
	"$rimlight" "1"		
	"$rimlightexponent" "4"				
	"$rimlightboost" "1.5"
	
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
	
	
	"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 70
		}
		
		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}

